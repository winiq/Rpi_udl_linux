# 1 "arch/arm64/boot/dts/overlays/edt-ft5406-overlay.dts"






/dts-v1/;
/plugin/;

# 1 "arch/arm64/boot/dts/overlays/edt-ft5406.dtsi" 1







/ {
 compatible = "brcm,bcm2835";

 fragment@10 {
  target = <&ft5406>;
  __overlay__ {
   touchscreen-inverted-x;
  };
 };

 fragment@11 {
  target = <&ft5406>;
  __overlay__ {
   touchscreen-inverted-y;
  };
 };

 fragment@12 {
  target = <&i2c_csi_dsi>;
  __overlay__ {
   #address-cells = <1>;
   #size-cells = <0>;
   ft5406: ts@38 {
    compatible = "edt,edt-ft5506";
    reg = <0x38>;

    touchscreen-size-x = < 800 >;
    touchscreen-size-y = < 480 >;
   };
  };
 };

 __overrides__ {
  sizex = <&ft5406>,"touchscreen-size-x:0";
  sizey = <&ft5406>,"touchscreen-size-y:0";
  invx = <0>, "-10";
  invy = <0>, "-11";
  swapxy = <&ft5406>,"touchscreen-swapped-x-y?";
 };
};
# 11 "arch/arm64/boot/dts/overlays/edt-ft5406-overlay.dts" 2

/ {
 fragment@0 {
  target = <&i2c0if>;
  __overlay__ {
   status = "okay";
  };
 };

 fragment@1 {
  target = <&i2c0mux>;
  __overlay__ {
   status = "okay";
  };
 };
};
