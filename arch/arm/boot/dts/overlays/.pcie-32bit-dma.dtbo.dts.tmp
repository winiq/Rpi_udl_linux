# 1 "arch/arm64/boot/dts/overlays/pcie-32bit-dma-overlay.dts"
# 1 "<built-in>"
# 1 "<command-line>"
# 1 "arch/arm64/boot/dts/overlays/pcie-32bit-dma-overlay.dts"




/dts-v1/;
/plugin/;

/ {
 compatible = "brcm,bcm2711";

 fragment@0 {
  target-path = "/aliases";
  __overlay__ {





   pcie0 = "";
  };
 };

 fragment@1 {
  target = <&pcie0>;
  __overlay__ {





   #address-cells = <3>;
   #size-cells = <2>;
   dma-ranges = <0x02000000 0x0 0x00000000 0x0 0x00000000
          0x0 0x80000000>;
  };
 };

};
