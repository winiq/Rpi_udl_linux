# 1 "arch/arm64/boot/dts/overlays/gpio-led-overlay.dts"
# 1 "<built-in>"
# 1 "<command-line>"
# 1 "arch/arm64/boot/dts/overlays/gpio-led-overlay.dts"
# 51 "arch/arm64/boot/dts/overlays/gpio-led-overlay.dts"
/dts-v1/;
/plugin/;

/ {
 compatible = "brcm,bcm2835";

 fragment@0 {

  target = <&gpio>;
  __overlay__ {
   led_pin: led_pins@19 {
    brcm,pins = <19>;
    brcm,function = <1>;
    brcm,pull = <0>;
   };
  };
 };
 fragment@1 {
  target-path = "/";
  __overlay__ {
   leds: leds@0 {
    compatible = "gpio-leds";
    pinctrl-names = "default";
    pinctrl-0 = <&led_pin>;
    status = "okay";

    led: led {
                   label = "myled1";
     gpios = <&gpio 19 0>;
                   linux,default-trigger = "none";
    };
   };
  };
 };

 __overrides__ {
  gpio = <&led>,"gpios:4",
               <&leds>,"reg:0",
               <&led_pin>,"brcm,pins:0",
               <&led_pin>,"reg:0";
  label = <&led>,"label";
  active_low = <&led>,"gpios:8";
  trigger = <&led>,"linux,default-trigger";
 };

};
